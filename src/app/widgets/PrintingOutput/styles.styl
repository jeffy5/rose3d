.output-btns-container
  display: flex
  align-items: center
  padding: 4px
  height: 28px
  border-radius: 14px
  background: #E4E4E4

  .output-btn
    display: inline-block
    flex: 1
    height: 20px
    line-height: 20px
    padding: 0
    background: transparent
    border-radius: 10px
    font-size: 12px
    border: none
    color: #3D3D3D

    &:hover
      background: #FFFFFF

    &[disabled]
      cursor: not-allowed
